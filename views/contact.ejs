<%- include partials/header.ejs %>

<%- include partials/main_nav.ejs %>
  

  <!-- Main Content -->
  <main class="container">
    <div class="jumbotron">
      <h1 class="display-4">Contact Us</h1>
    </div>
    <div class="container">
      <form action="/contact_us" method="POST">
    
        <label for="fname">First Name</label>
        <input type="text" id="fname" name="fname" placeholder="Your name.."><br>
    
        <label for="lname">Last Name</label>
        <input type="text" id="lname" name="lname" placeholder="Your last name.."><br>
    
        <label for="country">Phone Number</label>
        <input type="text" id="phone" name="phone" placeholder="Your Phone Number.."><br>

        <label for="email">Email</label>
        <input type="text" id="email" name="email" placeholder="Your email.."><br>
    
        <label for="message">Message</label>
        <textarea id="message" name="message" placeholder="Write something.." style="height:100px"></textarea><br>
    
        <input type="submit" value="Submit">
    
      </form>
      <br>  
    </div>
  </main>
  
<%- include partials/bottom_nav.ejs %>  

<%- include partials/footer.ejs %>